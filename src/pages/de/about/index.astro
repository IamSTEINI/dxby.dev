---
import Layout from "../../../layouts/Layout.astro";
import Navbar from "../../../components/Navbar";
import SlowType from "@/components/ui/SlowType";
import TextMarking from "../../../components/ui/TextMarking";
import Cypher from "@/components/ui/CypherText";
import RevealOnHover from "../../../components/ui/RevealOnHover";
---

<Layout>
  <main class="w-full h-full flex flex-col justify-center items-center">
    <div
      class="w-full max-w-[1000px] overflow-hidden flex h-fit flex-col items-center justify-start mt-10"
    >
      <div class="mt-10 sm:w-10/12 w-full pl-1 pr-1 h-fit flex flex-col justify-start">
        <span
          class="text-[35px] select-none sm:text-3xl relative w-full text-start font-bold tracking-wide text-nowrap"
        >
          <SlowType text="Über mich" typingSpeed={8000} client:load />
          <span
            class="absolute left-0 rotate-3 hover:-rotate-3 transition-all duration-75 ease-in-out cursor-pointer text-purple-400 bg-[#191919] text-center w-fit h-fit"
          >
            <SlowType
              text="Über mich"
              typingSpeed={8000}
              delay={1000}
              client:load
            />
          </span>
        </span>
        <span class="h-fit mt-5">
          <Cypher
            css="text-xl max-w-[700px]"
            text="Relativ jung, aus Deutschland, süchtig nach Musik. Die erste Code datei 2018 angefasst und bis jetzt viel neues gelernt."
            randomInterval={1}
            revealInterval={30}
            client:load
          />
        </span>
        <div class="mt-16 sm:mt-10">
          <TextMarking
            text="Skills"
            delay={0.7}
            speed={1}
            textcss="text-2xl font-bold italic"
            client:load
          />
        </div>
